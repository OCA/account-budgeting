<odoo>

  <record id="budget_control_expenses" model="mis.report">
    <field name="name">Budget Expenses</field>
  </record>

  <record id="budget_control_expenses_kpi_exp" model="mis.report.kpi">
    <field name="report_id" ref="budget_control_expenses"/>
    <field name="name">exp</field>
    <field name="description">Expenses</field>
    <field name="auto_expand_accounts">True</field>
    <field name="budgetable" eval="True"/>
    <field name="sequence">1</field>
  </record>

  <record id="budget_control_expenses_kpi_expr_exp" model="mis.report.kpi.expression">
    <field name="kpi_id" ref="budget_control_expenses_kpi_exp"/>
    <field name="name">balp[220000]</field>
  </record>

  <record id="budget_control_expenses_kpi_total" model="mis.report.kpi">
    <field name="report_id" ref="budget_control_expenses"/>
    <field name="name">total</field>
    <field name="description">Total</field>
    <field name="expression">exp</field>
    <field name="auto_expand_accounts">False</field>
    <field name="budgetable" eval="False"/>
    <field name="sequence">4</field>
  </record>

</odoo>
